ate: date\n    })\n    this.feedUpdate()\n  }\n  \n  handleRegister = (email, password) => {\n    auth.createUserWithEmailAndPassword(email, password)\n      .then(user => {\n        db.collection('users').add({\n          email: user.user.email,\n          uid: user.user.uid,\n          username: this.state.username\n        })\n          .then(() => this.goTo('home'))\n      })\n      .catch(err => {\n        console.log(err.code)\n        console.log(err.message)\n      })\n  }\n\n  feedUpdate = () => {\n    let array = []\n    db.collection('fweets').orderBy('date').onSnapshot(querySnapshot => {\n      querySnapshot.docs.map(fweet => {\n        if(!array.includes(fweet.data())) {\n          array.push(fweet.data())\n        }\n      })\n      this.setState({ fweets: array.reverse() })\n    })\n  }\n\n  componentDidMount() {\n    this.feedUpdate()\n  }\n  \n  handleSignin = (email, password) => {\n    auth.signInWithEmailAndPassword(email, password)\n      .then(user => {\n        db.collection('users').where('uid', '==', user.user.uid).onSnapshot(querySnapshot => {\n          querySnapshot.docs.map(user => {\n            this.setState({ username: user.data().username })\n          })\n        })\n        this.goTo('home')\n      })\n      .catch(err => {\n        console.log(err.message)\n      })\n  }\n\n  handleLogout = () => {\n    auth.signOut()\n    this.goTo('signin')\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        {\n          this.state.route === 'home' ? (\n            <Fragment>\n              <Header handleLogout={this.handleLogout} username={this.state.username}/>\n              <PostTweet handleChange={this.handleChange} handlePostTweet={this.handlePostTweet} value={this.state.fweet}/>\n              <Feed fweets={this.state.fweets}/>\n            </Fragment>\n          ) : (\n            this.state.route === 'register' ? (\n              <Register handleRegister={() => this.handleRegister(this.state.email, this.state.password)} goToSignin={() => this.goTo('signin')} handleSetEmail={this.handleSetEmail} handleSetPassword={this.handleSetPassword} handleSetUsername={this.handleSetUsername}/>\n            ) : (\n              <SignIn handleSignin={() => this.handleSignin(this.state.email, this.state.password)} goToRegister={() => this.goTo('register')} handleSetEmail={this.handleSetEmail} handleSetPassword={this.handleSetPassword}/>\n            )\n          )\n        }\n      </div>\n    )\n  }\n}\n\nexport default App;\n","/home/colinblk/Documents/BOTW/twitter-clone/src/components/Header/Header.js",["48","49","50"],"import React from 'react'\nimport './header.css'\n\nconst Header = ({ handleLogout, username }) => {\n    return (\n        <header className='header'>\n            <a href='#' className='header-title'>Fwitter</a>\n            <nav className='navlinks'>\n                <a href='#' className='navlink'>{username}</a>\n                <a href='#' className='navlink' onClick={handleLogout}>Log Out</a>\n            </nav>\n        </header>\n    );\n};\n\nexport default Header;","/home/colinblk/Documents/BOTW/twitter-clone/src/tweets.js",[],"/home/colinblk/Documents/BOTW/twitter-clone/src/components/PostTweet/PostTweet.js",[],"/home/colinblk/Documents/BOTW/twitter-clone/src/components/Feed/Feed.js",[],"/home/colinblk/Documents/BOTW/twitter-clone/src/components/Register/Register.js",["51"],"import React from 'react'\nimport './register.css'\n\nconst Register = ({ handleRegister, goToSignin, handleSetEmail, handleSetPassword, handleSetUsername }) => {\n    return (\n        <div className='register-form-container'>\n            <h1>Register</h1>\n            <form className='register-form' onSubmit={handleRegister}>\n                <label htmlFor=\"username-input-register\">Username</label>\n                <input type=\"text\" placeholder='Username' id='username-input-register' className='register-input' onChange={handleSetUsername} required/>\n                <label htmlFor=